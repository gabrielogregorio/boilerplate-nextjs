{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "prefer-const": "error",
    "camelcase": "error"
  },

  // resolve message 'WARNING: You are currently running a version of TypeScript which is not officially supported by @typescript-eslint/typescript-estree.'
  // https://github.com/facebook/create-react-app/issues/7160#issuecomment-502631002
  "parserOptions": {
    "warnOnUnsupportedTypeScriptVersion": false
  }
}
